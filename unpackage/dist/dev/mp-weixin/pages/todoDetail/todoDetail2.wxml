<view class="box"><view class="title">党建翼联对象</view><view class="uni-form"><view class="uni-form-item"><view class="label">党组织名称</view><view class="content">{{deatailInfo.unitOrgName}}</view></view><view class="uni-form-item"><view class="label">结对类型</view><view class="content">{{''+deatailInfo.pairType+''}}</view></view><view class="uni-form-item"><view class="label">对象名称</view><view class="content">{{''+deatailInfo.objName+''}}</view></view><view class="uni-form-item"><view class="label">对象类型</view><view class="content">{{''+deatailInfo.objType+''}}</view></view><view class="uni-form-item"><view class="label">签约时间</view><view class="content">{{''+deatailInfo.firstSignTime+''}}</view></view><view class="uni-form-item"><view class="label">签约书</view><block wx:for="{{deatailInfo.singleFile}}" wx:for-item="item" wx:for-index="__i0__" wx:key="dataId"><view data-event-opts="{{[['tap',[['toViewFile',['$0'],[[['deatailInfo.singleFile','dataId',item.dataId]]]]]]]}}" class="content" bindtap="__e"><view class="iconfont icon-pdf _i"></view><text class="file-pdf">{{item.fileName}}</text></view></block></view></view><block wx:if="{{todoStatus==='1'}}"><view class="btn-box"><button data-event-opts="{{[['tap',[['submit',['reject']]]]]}}" class="btn-reject" bindtap="__e">驳回</button><button data-event-opts="{{[['tap',[['submit',['adopt']]]]]}}" class="btn-adopt" bindtap="__e">通过</button></view></block></view>