<view><view class="tab-list-box"><view class="tab-list"><block wx:for="{{tabList}}" wx:for-item="item" wx:for-index="index" wx:key="type"><view data-event-opts="{{[['tap',[['changeTab',['$0'],[[['tabList','type',item.type]]]]]]]}}" class="{{['tab-item',reqFlag===item.type?'active':'']}}" bindtap="__e">{{''+item.title+''}}</view></block></view></view><view class="tab-content-box"><block wx:if="{{activityList&&activityList.length>0}}"><view class="tab-content-list"><block wx:for="{{activityList}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><view class="tab-content-item"><view class="content-left"><view class="name">{{item.title}}</view><view class="time">{{item.activityTime}}</view><view class="address">{{item.place}}</view></view><view class="content-right"><block wx:if="{{item.checkInStatus==='请假'}}"><button data-event-opts="{{[['tap',[['scanCode']]]]}}" class="btn-scan" bindtap="__e"><view class="iconfont icon-iconfontscan _i"></view>扫码签到</button><button data-event-opts="{{[['tap',[['toLeave',['$0'],[[['activityList','id',item.id]]]]]]]}}" class="btn-leave" bindtap="__e">请假</button></block><block wx:if="{{item.checkInStatus==='请假中'}}"><view class="leave-img"><image src="../../static/images/leaving.png" mode="widthFix"></image></view></block><block wx:if="{{item.checkInStatus==='按时签到'}}"><button class="btn-on-time"><view class="iconfont icon-duihao _i"></view>按时签到</button><view class="date-on-time">{{item.checkInTime}}</view></block><block wx:if="{{item.checkInStatus==='延迟签到'}}"><button class="btn-delay"><view class="iconfont icon-duihao _i"></view>延时签到</button><view class="date-delay">{{item.checkInTime}}</view></block><block wx:if="{{item.checkInStatus==='已请假'}}"><view class="leaved-img"><image src="../../static/images/leaved.png" mode="widthFix"></image></view></block></view></view></block></view></block><block wx:else><no-data vue-id="0a9b32a0-1" bind:__l="__l"></no-data></block><block wx:if="{{isEnd}}"><view class="is-end">我是有底线的</view></block></view><block wx:if="{{showPopup}}"><view class="mask-wrapper"><view class="mask-bg"></view><view class="mask-content"><image src="{{computedImg}}" mode="widthFix"></image><view data-event-opts="{{[['tap',[['closePopup',['$event']]]]]}}" class="popup-close" bindtap="__e"><image src="../../static/images/popup-close1.png" mode="widthFix"></image></view><view class="main-text"><view class="title">{{computedTitle}}</view><view class="sign-time">签到时间：2021-07-26 12:25</view><view class="start-time">2021-07-26  12:30</view></view></view></view></block></view>