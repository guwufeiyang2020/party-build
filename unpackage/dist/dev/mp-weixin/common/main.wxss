/*每个页面公共css */
view,
scroll-view,
swiper,
swiper-item,
cover-view,
cover-image,
icon,
text,
rich-text,
progress,
button,
checkbox,
form,
input,
label,
radio,
slider,
switch,
textarea,
navigator,
audio,
camera,
image,
video {
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
image {
	width: 100%;
	height: 100%;
}
/* 公共组件样式 */
/* 单行溢出隐藏 */
.text-ellipsis {
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}
.is-end {
	margin-top: 20rpx;
	line-height: 38rpx;
	text-align: center;
	color: #aab2bf;
	font-size: 12px;
}
/* tab */
.tab-list-box {
	width: 100%;
	height: 256rpx;
	padding: 50rpx 30rpx 115rpx;
	background-color: #da2a2a;
}
.tab-list {
	width: 100%;
	height: 92rpx;
	background-color: rgba(255, 255, 255, 0.4);
	border-radius: 46rpx;
	padding: 0 10rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.tab-item {
	font-size: 32rpx;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	height: 70rpx;
	text-align: center;
	border-radius: 35rpx;
	line-height: 70rpx;
	color: #fff;
}
.tab-item.active {
	background-color: #fff;
	color: #333;
}
.tab-content-box {
	position: relative;
	top: -62rpx;
	width: 100%;
	padding: 20rpx 40rpx 0;
	background-color: #fff;
	border-radius: 30rpx 30rpx 0 0;
}

